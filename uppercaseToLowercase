class Main {
    public static void main(String[] args) {
        String s="ANUPRIYA";
        String str="";
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)>=97 || s.charAt(i)<=122){
                str=str+(char)(s.charAt(i)+32);
            }
        }
        System.out.println(str);
    }
}
