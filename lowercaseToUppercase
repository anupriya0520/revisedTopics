class Main {
    public static void main(String[] args) {
        String s="anupriya";
        String str="";
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)>=65 || s.charAt(i)<=90){
                str=str+(char)(s.charAt(i)-32);
            }
        }
        System.out.println(str);
    }
}
