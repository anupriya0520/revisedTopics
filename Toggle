class Main {
    public static void main(String[] args) {
        String s="ANUpriya";
        String str="";
        for(int i=0;i<s.length();i++){
            char ch=s.charAt(i);
            if(Character.isUpperCase(ch)){
                str+=Character.toLowerCase(ch);
            }else if(Character.isLowerCase(ch)){
                str+=Character.toUpperCase(ch);
            }else{
                str+=ch;
            }
        }
        System.out.println(str);
    }
}
